using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace DapperHC.Models
{
    public class HCModel
    {
        public string Depatment部门 { get; set; }
        public string Shift班次 { get; set; }
        public string Area区域 { get; set; }
        public string Team班组 { get; set; }
        public string Position职位 { get; set; }
        public string Category类别 { get; set; }
        public string Basis基准 { get; set; }
        public string Contract合同 { get; set; }
        public DateTime Date发生时间 { get; set; }
        public float HC编制 { get; set; }
        public int HCID岗位编号 { get; set; }
    }

}
